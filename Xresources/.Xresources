! X colors.
! Generated by 'wal'
emacs*foreground:   #FFFFFF
emacs*background:   #504e45
URxvt*foreground:   #FFFFFF
XTerm*foreground:   #FFFFFF
UXTerm*foreground:  #FFFFFF
URxvt*background:   [100]#504e45
XTerm*background:   #504e45
UXTerm*background:  #504e45
URxvt*cursorColor:  #FFFFFF
XTerm*cursorColor:  #FFFFFF
UXTerm*cursorColor: #FFFFFF
URxvt*borderColor:  [100]#504e45

! Colors 0-15.
*.color0: #504e45
*color0:  #504e45
*.color1: #5DA0FC
*color1:  #5DA0FC
*.color2: #6CA9FE
*color2:  #6CA9FE
*.color3: #88B8FC
*color3:  #88B8FC
*.color4: #A7C2D6
*color4:  #A7C2D6
*.color5: #D3E4EE
*color5:  #D3E4EE
*.color6: #E9F3F6
*color6:  #E9F3F6
*.color7: #FFFFFF
*color7:  #FFFFFF
*.color8: #96948f
*color8:  #96948f
*.color9: #5DA0FC
*color9:  #5DA0FC
*.color10: #6CA9FE
*color10:  #6CA9FE
*.color11: #88B8FC
*color11:  #88B8FC
*.color12: #A7C2D6
*color12:  #A7C2D6
*.color13: #D3E4EE
*color13:  #D3E4EE
*.color14: #E9F3F6
*color14:  #E9F3F6
*.color15: #FFFFFF
*color15:  #FFFFFF

! Black color that will not be affected by bold highlighting.
*.color66: #504e45
*color66:  #504e45

! Rofi colors.
rofi.color-window: #504e45, #504e45, #6CA9FE
rofi.color-normal: #504e45, #FFFFFF, #504e45, #6CA9FE, #504e45
rofi.color-active: #504e45, #FFFFFF, #504e45, #6CA9FE, #504e45
rofi.color-urgent: #504e45, #5DA0FC, #504e45, #5DA0FC, #FFFFFF

! Xclock colors.
XClock*foreground: #FFFFFF
XClock*background: #504e45
XClock*majorColor:  rgba:ff/ff/ff/ff
XClock*minorColor:  rgba:ff/ff/ff/ff
XClock*hourColor:   rgba:ff/ff/ff/ff
XClock*minuteColor: rgba:ff/ff/ff/ff
XClock*secondColor: rgba:ff/ff/ff/ff

!URxvt*font: xft:Roboto:size=13, xft:PowerlineSymbols:size=13
URxvt*font: xft:Iosevka Nerd Font:size=10
!URxvt*font: xft:Source Code Pro for Powerline:size=10

! Set depth to make transparency work.
URxvt*depth: 32

!Disable scrollbar
URxvt.scrollBar: false

!Dont scroll URxvt with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
